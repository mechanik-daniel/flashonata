{
  "name": "flashteval",
  "version": "1.0.0",
  "description": "FLASH Transformation Evaluator (FlashTEval) is FUME's mapping language parser and executor.",
  "module": "flashteval.js",
  "main": "flashteval.js",
  "typings": "flashteval.d.ts",
  "scripts": {
    "fume-test": "node src/fume.test.js",
    "pretest": "npm run lint",
    "mocha": "nyc ./node_modules/mocha/bin/_mocha -- \"test/**/*.js\" --timeout=5000",
    "test": "npm run mocha",
    "--posttest": "npm run check-coverage && npm run browserify && npm run minify && npm run build-es5",
    "build-es5": "npm run mkdir-dist && npm run regenerator && npm run browserify-es5 && npm run minify-es5",
    "check-coverage": "nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "browserify": "browserify src/flashteval.js --outfile flashteval.js --standalone flashteval",
    "mkdir-dist": "mkdirp ./dist",
    "regenerator": "babel src --out-dir dist --presets=@babel/env",
    "browserify-es5": "regenerator --include-runtime polyfill.js > flashteval-es5.js; browserify dist/flashteval.js --standalone flashteval >> flashteval-es5.js",
    "prepublishOnly": "npm run browserify && npm run minify && npm run build-es5",
    "lint": "eslint src",
    "doc": "jsdoc --configure jsdoc.json .",
    "cover": "nyc _mocha",
    "minify": "uglifyjs flashteval.js -o flashteval.min.js --compress --mangle",
    "minify-es5": "uglifyjs flashteval-es5.js -o flashteval-es5.min.js --compress --mangle"
  },
  "license": "MIT",
  "keywords": [
    "JSON",
    "query",
    "transformation",
    "transform",
    "mapping",
    "path"
  ],
  "devDependencies": {
    "@babel/cli": "^7.27.2",
    "@babel/core": "^7.27.4",
    "@babel/preset-env": "^7.27.2",
    "@outburn/structure-navigator": "^1.0.2",
    "browserify": "^16.5.2",
    "chai": "^4.5.0",
    "chai-as-promised": "^7.1.2",
    "eslint": "8.0.0",
    "eslint-plugin-ideal": "^0.1.3",
    "eslint-plugin-promise": "^6.6.0",
    "jsdoc": "^4.0.4",
    "mkdirp": "^1.0.4",
    "mocha": "^11.7.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^15.1.0",
    "regenerator": "^0.14.12",
    "request": "^2.88.2",
    "uglify-es": "^3.3.10"
  },
  "engines": {
    "node": ">= 8"
  }
}
